run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]
modules = ["nodejs-20", "python-3.11", "postgresql-16"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[env]
NODE_ENV = "development"
REPLIT_KEEP_PACKAGE_DEV_DEPENDENCIES = "1"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 36555
externalPort = 9000

[[ports]]
localPort = 41507
externalPort = 8080

[[ports]]
localPort = 45479
externalPort = 8099

[[ports]]
localPort = 45943
externalPort = 8081

[agent]
integrations = ["sendgrid:1.0.0", "javascript_gemini_ai_integrations:1.0.0"]
