run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]
modules = ["nodejs-20", "python-3.11", "postgresql-16"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[env]
NODE_ENV = "development"
REPLIT_KEEP_PACKAGE_DEV_DEPENDENCIES = "1"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 5001
externalPort = 80

[agent]
integrations = ["sendgrid:1.0.0", "javascript_gemini_ai_integrations:1.0.0"]

[userenv]

[userenv.shared]
BREVO_SENDER_EMAIL = "info@warubi-sports.com"
BREVO_SENDER_NAME = "Warubi Sports"
